alias gp="git pull"
alias gpu="git push"
alias gpf="git push -f"
alias gs="git status"
alias gst="git stash"
alias ga="git add"
alias gaa="git add *"
alias gaac="git add * && git commit -m"
alias gc="git commit -m"
alias gca="git commit --amend"
alias gl="git log"
alias glagd="git log --all --graph --decorate"
alias glagdo="git log --all --graph --decorate --online"
alias grrh="git reset --hard HEAD"
