[init]
	defaultBranch = main
[user]
	email = 5432978+krparajuli@users.noreply.github.com
	name = Kalyan Parajuli
[push]
	autoSetupRmote = true
	autoSetupRemote = true
[advice]
	addIgnoredFile = false
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[pull]
	rebase = false
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
[merge]
	conflictStyle = zdiff3
